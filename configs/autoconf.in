MESA_LLVM = @MESA_LLVM@

LLVM_VERSION = @LLVM_VERSION@
ifneq ($(LLVM_VERSION),)
  HAVE_LLVM := 0x0$(subst .,0,$(LLVM_VERSION:svn=))
  DEFINES += -DHAVE_LLVM=$(HAVE_LLVM)
endif

HAVE_XF86VIDMODE = @HAVE_XF86VIDMODE@
